{
	"module": "commonjs",
	"target": "es5",
	"lib": ["es2015", "dom"],
	"outDir": "lib",
	"sourceMap": true,
	"declaration": true,
	"compilerOptions": {
		"baseUrl": "./",
		"strict": true,
		"target": "ES5",
		"importHelpers": true,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "preserve"
	},
	"exclude": ["node_modules", "dist"]
}
